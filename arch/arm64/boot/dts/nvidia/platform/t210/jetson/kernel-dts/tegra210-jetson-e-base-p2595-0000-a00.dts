				status = "okay";
				mode = "host";
			};
			usb2-2 {
				status = "okay";
				mode = "host";
			};
			usb3-0 {
				status = "okay";
				nvidia,usb2-companion = <1>;
			};
			usb3-1 {
				status = "okay";
				nvidia,usb2-companion = <2>;
			};
			hsic-0 {
				status = "okay";
			};
		};
